{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "types": [
      "next",
      "@types/node",
    ],
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*",
        "./app/*",
      ],
      
    }
  },
  "include": [
    "**/*.ts",
    ".next/types/**/*.ts",
    "next-env.d.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx"
, "app/components/CustomTable"  ],
  "exclude": [
    "node_modules",
    "email-templates",
    "email-templates/**/*",
    "tailwindui",
    "./tailwindui",
    "tailwindui/**/*",
    "./.history",
    "/.history",
    "./.history",
    "./certs",
    "certs",

  ]
}
